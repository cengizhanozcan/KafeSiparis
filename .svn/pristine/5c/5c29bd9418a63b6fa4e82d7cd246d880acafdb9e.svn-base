package model;

/**
 *
 * @author cengizhan
 */
public class Deneme {
    
    private int id;
    private String ad;
    private String soyad;
    
    public Deneme(){}

    
    public Deneme(int id, String ad, String soyad){
    
        this.id = id;
        this.ad = ad;
        this.soyad = soyad;
    }
    
    
    /**
     * @return the id
     */
    public int getId() {
        return id;
    }

    /**
     * @param id the id to set
     */
    public void setId(int id) {
        this.id = id;
    }

    /**
     * @return the ad
     */
    public String getAd() {
        return ad;
    }

    /**
     * @param ad the ad to set
     */
    public void setAd(String ad) {
        this.ad = ad;
    }

    /**
     * @return the soyad
     */
    public String getSoyad() {
        return soyad;
    }

    /**
     * @param soyad the soyad to set
     */
    public void setSoyad(String soyad) {
        this.soyad = soyad;
    }
    
}
